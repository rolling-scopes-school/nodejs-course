{
  "root": true,
  "env": {
    "es2020": true,
    "jasmine": true,
    "jest": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "settings": {
    "noInlineConfig": true
  },
  "extends": ["auto"],
  "rules": {
    "import/no-named-as-default-member": "off",
    "unicorn/prefer-module": "off"
  }
}
